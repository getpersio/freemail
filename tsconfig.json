{
  "compilerOptions": {
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "inlineSources": true,
    "inlineSourceMap": true,
    "target": "es5",
    "outDir": "./",
    "rootDir": "./src",
    "module": "commonjs",
    "declaration": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "lib": ["es5"],
    "resolveJsonModule": true
  },
  "include": [
  "lib/**/*.ts",
  "src/index.ts"
   ],
  "exclude": [
     "test/**/*.ts"
   ]
}